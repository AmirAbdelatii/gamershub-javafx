<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="530.0" prefWidth="832.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gamershub.Gui.ListBlogBackController">
   <children>
      <VBox layoutX="-1.0" layoutY="1.0" prefHeight="530.0" prefWidth="832.0" />
      <ListView fx:id="tableBlog" layoutX="83.0" layoutY="74.0" prefHeight="372.0" prefWidth="705.0" />
      <Button layoutX="111.0" layoutY="473.0" mnemonicParsing="false" onAction="#deletePost" prefHeight="31.0" prefWidth="157.0" style="-fx-background-color: #F0882F; -fx-background-radius: 15px; -fx-border-radius: 15px; -fx-cursor: hand;" text="Delete Post" textFill="WHITE">
         <font>
            <Font name="Segoe UI Semibold" size="17.0" />
         </font>
      </Button>
      <Button fx:id="btnUpdatePost" layoutX="601.0" layoutY="473.0" mnemonicParsing="false" onAction="#updatePost" prefHeight="31.0" prefWidth="157.0" style="-fx-background-color: #F0882F; -fx-background-radius: 15; -fx-border-radius: 15; -fx-cursor: hand;" text="Update Post" textFill="WHITE">
         <font>
            <Font name="Segoe UI Semibold" size="17.0" />
         </font>
      </Button>
      <Button fx:id="btnAddPost" layoutX="362.0" layoutY="473.0" mnemonicParsing="false" onAction="#addPost" prefHeight="31.0" prefWidth="157.0" style="-fx-background-color: #F0882F; -fx-background-radius: 15; -fx-border-radius: 15; -fx-cursor: hand;" text="Add Post" textFill="WHITE">
